#include<iostream>
using namespace std;

int main() {

    int i=1;
    int n;
    cin>>n;

    while(i<=n){
        int j=1;
        
        while(j<=n){
            cout<<j;
            j++;
            if(j>i){
                break;
            }
        }
        cout<<endl;
        i++;
    }

    return 0;
}
