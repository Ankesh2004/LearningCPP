#include<iostream>
using namespace std;

void sortingArray(int a[], int n){
     for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            int temp;
            int min=i;
            if(a[i]<a[j]){
                min=j;
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;

            }
            else{
                continue;
            }
        }
     }
}

void binarySearch(int a[], int n, int x){
     int m=n/2;

   if(a[m]<x){
            for(int p=m;p<=(n-1);p++){
                if(a[p]==x){
                    cout<<p<<endl;
                }
                else{
                    
                    continue;

                }
            }
        }

    if(a[m]>x){
            for(int p=m;p>=0;p--){
                if(a[p]==x){
                    cout<<p<<endl;
                }
                else{
                    continue;
                }
            }
     }
    if(a[m]==x){
        cout<<m<<endl;
    }

    else if((a[m]!=x)&&(a[m]<x)&&(a[m]>x)){
        cout<<-1<<endl;
    }  

}

int main() {

    int n,x;
    cin>>n;
    int a[100];

    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    cin>>x;
    
    sortingArray(a,n);
    binarySearch(a,n,x); 
    return 0;

} 
