#include<iostream>
using namespace std;

 int fib(int n){
    if(n>1){
    return fib(n-1)+fib(n-2);
    }
    else if(n==0){
        return 0;
    }

    else if(n==1){
        return 1;
    }
}

int main() {

    int N,i;
    cout<<"enter the number"<<endl;
    cin>>N;

    for(i=0;fib(i)<=N;i++){
        cout<<fib(i)<<endl;
    }

    return 0;
}
