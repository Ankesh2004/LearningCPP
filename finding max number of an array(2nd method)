#include<iostream>
using namespace std;

int main() {
int n,i,max,j;
cout<<"How many numbers to compare?"<<endl;
    cin>>n;

    cout<<"Enter "<<n<<" numbers:"<<endl;
    int num[n];
    
    for(i=0;i<n;i++){
        cin>>num[i];
    }

    max=0

    for(j=0;j<n;j++){
        if(max<num[j]){
            max=num[j];
        }

        else{
            continue;
        }
    }

    cout<<"The maximum number is = "<<max<<endl;

    return 0;
}
