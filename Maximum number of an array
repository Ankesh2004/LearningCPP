#include<iostream>
using namespace std;

int main() {
int n,i,m,j;
cout<<"How many numbers to compare?"<<endl;
    cin>>n;

    cout<<"Enter "<<n<<" numbers:"<<endl;
    int num[n];
    
    for(i=0;i<n;i++){
        cin>>num[i];
    }

    m=0;

    for(j=0;j<n;j++){
        m= (max(num[j],m));

    }

    cout<<"The maximum number is = "<<m<<endl;


    return 0;
}
